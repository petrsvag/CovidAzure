{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Název datové továrny",
			"defaultValue": "CovidDataFactoryV2"
		},
		"CovidHTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://onemocneni-aktualne.mzcr.cz"
		},
		"containerUri": {
			"type": "string",
			"metadata": "Identifikátor URI k účtu úložiště obsahujícímu propojené šablony ARM"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token SAS pro účet úložiště, který obsahuje propojené šablony ARM"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "CovidDataFactoryV2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"CovidHTTP_properties_typeProperties_url": {
						"value": "[parameters('CovidHTTP_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}