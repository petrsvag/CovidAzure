{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "CovidDataFactoryV2"
		},
		"AzureBlobStorageCovidKraj_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageCovidKraj'"
		},
		"CovidAzureDatabricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'CovidAzureDatabricks'"
		},
		"CovidHTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://onemocneni-aktualne.mzcr.cz"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "CovidDataFactoryV2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageCovidKraj_connectionString": {
						"value": "[parameters('AzureBlobStorageCovidKraj_connectionString')]"
					},
					"CovidAzureDatabricks_accessToken": {
						"value": "[parameters('CovidAzureDatabricks_accessToken')]"
					},
					"CovidHTTP_properties_typeProperties_url": {
						"value": "[parameters('CovidHTTP_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}